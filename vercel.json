{
  "framework": "remix",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
