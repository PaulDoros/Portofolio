{
  "framework": "remix",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      }
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
